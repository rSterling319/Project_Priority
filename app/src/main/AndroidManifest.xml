<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rs.projectpriority">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DisplayProjectsActivity"
            android:parentActivityName=".MainActivity"
            android:label="Your Projects">

        </activity>
        <activity
            android:name=".ProjectActivity"
            android:parentActivityName=".DisplayProjectsActivity"
            android:label="Project Name">
        </activity>
        <activity
            android:name=".NotesActivity"
            android:parentActivityName=".MainActivity"
            android:label="Notes">
        </activity>
        <activity
            android:name=".DisplayToDoListActivity"
            android:parentActivityName=".MainActivity"
            android:label="Your To-Do List">
        </activity>

    </application>

</manifest>